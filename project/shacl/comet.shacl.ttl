            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comet:rights ],
        [ sh:class comet:Agent ;
            sh:description "A person or organization that contributed to the creation of the work" ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path dcterms:contributor ],
        [ sh:class comet:Agent ;
            sh:description "The person or organization who created the work" ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path dcterms:creator ],
