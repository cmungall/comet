            sh:description "The title of the item" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comet:title ],
        [ sh:datatype xsd:string ;
            sh:description "Keywords or tags used to describe this item" ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path schema1:keywords ] ;
